# ===============================================
# Bitwarden Helper Functions
# ===============================================

# Salvar um token/API key no Bitwarden
# Uso: bw-set "Nome do Token" "ghp_xxxxxxxxxxxx" "Descri√ß√£o opcional"
bw-set() {
    if [[ -z "$1" || -z "$2" ]]; then
        echo "‚ùå Uso: bw-set 'Nome' 'token_value' 'descri√ß√£o opcional'"
        return 1
    fi

    local name="$1"
    local token="$2"
    local description="${3:-Token/API Key}"

    cat << EOF | bw encode | bw create item
{
  "type": 2,
  "name": "$name",
  "notes": "$description",
  "secureNote": {
    "type": 0
  },
  "fields": [
    {
      "name": "token",
      "value": "$token",
      "type": 0
    },
    {
      "name": "created",
      "value": "$(date +%Y-%m-%d)",
      "type": 0
    }
  ]
}
EOF
    echo "‚úÖ Token '$name' salvo com sucesso!"
    bws  # Sincroniza automaticamente
}

# Pegar um token do Bitwarden
# Uso: bw-get "Nome do Token"
bw-get() {
    if [[ -z "$1" ]]; then
        echo "‚ùå Uso: bw-get 'Nome do Token'"
        return 1
    fi

    # Use the mkcert root CA from keite-guica server for SSL verification
    NODE_EXTRA_CA_CERTS=$HOME/.certs/keite-guica-rootCA.pem bw get item "$1" | jq -r '.fields[] | select(.name=="token") | .value'
}

# Copiar token para clipboard
# Uso: bw-copy "Nome do Token"
bw-copy() {
    if [[ -z "$1" ]]; then
        echo "‚ùå Uso: bw-copy 'Nome do Token'"
        return 1
    fi

    local token=$(bw-get "$1")
    if [[ -n "$token" ]]; then
        echo -n "$token" | pbcopy
        echo "‚úÖ Token copiado para o clipboard!"
    else
        echo "‚ùå Token n√£o encontrado"
        return 1
    fi
}


